<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>OWL Ontology Converter Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ConvertOntologyServlet</servlet-name>
    <servlet-class>eu.fbk.irst.dkm.owlapi.converter.servlet.ConvertOntologyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConvertOntologyServlet</servlet-name>
    <url-pattern>/convert</url-pattern>
  </servlet-mapping>
</web-app>
